<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.animeapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_anime"
        android:label="FB Animes"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AnimeApp">
        <activity
            android:name=".Page_OnePiece"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Page_DemonSlayer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Page_Neverland"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Page_BokuNoHero"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RegistroActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>